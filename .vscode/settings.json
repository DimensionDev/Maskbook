{
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.displayLanguage": "en",
    "i18n-ally.localesPaths": "packages/maskbook/src/_locales",
    "i18n-ally.sortKeys": true,
    "i18n-ally.keystyle": "flat",
    "i18n-ally.annotations": true,
    "i18n-ally.indent": 4,
    "i18n-ally.enabledParsers": ["json"],
    "json.schemas": [
        {
            "fileMatch": ["/packages/maskbook/src/manifest.json"],
            "url": "http://json.schemastore.org/chrome-manifest"
        },
        {
            "fileMatch": ["/packages/maskbook/src/plugins/*/schema.json"],
            "url": "http://json-schema.org/draft-07/schema"
        },
        {
            "fileMatch": ["/packages/maskbook/src/_locales/*/messages.json"],
            "url": "/.vscode/schema/locale.json"
        }
    ],
    "cSpell.words": ["Reificated"]
}
