{
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "json.schemas": [
        {
            "fileMatch": ["/packages/maskbook/src/manifest.json"],
            "url": "http://json.schemastore.org/chrome-manifest"
        },
        {
            "fileMatch": ["/packages/maskbook/src/plugins/*/schema.json"],
            "url": "http://json-schema.org/draft-07/schema"
        },
        {
            "fileMatch": ["/packages/maskbook/src/_locales/*/messages.json"],
            "url": "/.vscode/schema/locale.json"
        }
    ],
    "cSpell.words": ["Reificated"]
}
