{
  "name": "@masknet/plugin-template",
  "private": true,
  "sideEffects": [
    "./src/register.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "mask-src": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./register": {
      "mask-src": "./src/register.ts",
      "default": "./dist/register.js"
    }
  },
  "scripts": {
    "lingui-extract": "../../../node_modules/.bin/lingui extract",
    "lingui-compile": "../../../node_modules/.bin/lingui compile"
  },
  "dependencies": {
    "@masknet/plugin-infra": "workspace:^",
    "@masknet/shared-base": "workspace:^"
  },
  "lingui": {
    "catalogs": [{ "path": "src/locale/{locale}", "include": ["src"] }],
    "compileNamespace": "json",
    "locales": ["en-US", "ja-JP", "ko-KR", "zh-CN", "zh-TW"]
  }
}
