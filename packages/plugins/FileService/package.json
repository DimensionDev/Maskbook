{
  "name": "@masknet/plugin-file-service",
  "private": true,
  "sideEffects": [
    "./src/register.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "mask-src": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./register": {
      "mask-src": "./src/register.ts",
      "default": "./dist/register.js"
    }
  },
  "dependencies": {
    "@dimensiondev/common-protocols": "1.6.0-20210723072221-8f46a92",
    "@masknet/icons": "workspace:^",
    "@masknet/plugin-infra": "workspace:^",
    "@masknet/shared": "workspace:^",
    "@masknet/shared-base": "workspace:^",
    "@masknet/shared-base-ui": "workspace:^",
    "@masknet/theme": "workspace:^",
    "@masknet/typed-message": "workspace:^",
    "@masknet/typed-message-react": "workspace:^",
    "@masknet/web3-providers": "workspace:^",
    "@masknet/web3-telemetry": "workspace:^",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@servie/events": "^3.0.0",
    "arweave": "^1.13.6",
    "date-fns": "^3.6.0",
    "ipfs-http-client": "^60.0.1",
    "react-router-dom": "^6.24.0",
    "react-use": "^17.5.0",
    "urlcat": "^3.1.0",
    "use-subscription": "^1.8.0"
  },
  "devDependencies": {
    "@types/use-subscription": "^1.0.2"
  },
  "lingui": {
    "compileNamespace": "json",
    "locales": [
      "en-US",
      "ja-JP",
      "ko-KR",
      "zh-CN",
      "zh-TW"
    ],
    "fallbackLocales": {
      "zh-CN": "zh-TW",
      "zh-TW": "zh-CN",
      "default": "en-US"
    },
    "formatOptions": {
      "origins": true,
      "lineNumbers": false
    },
    "catalogs": [
      {
        "path": "src/locale/{locale}",
        "include": [
          "src"
        ]
      }
    ]
  }
}
