{
  "name": "@masknet/plugin-infra",
  "private": true,
  "sideEffects": false,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "mask-src": "./src/entry.ts",
      "default": "./dist/entry.js"
    },
    "./dom": {
      "types": "./dist/entry-dom.d.ts",
      "mask-src": "./src/entry-dom.ts",
      "default": "./dist/entry-dom.js"
    },
    "./dom/context": {
      "types": "./dist/dom/context.d.ts",
      "mask-src": "./src/dom/context.ts",
      "default": "./dist/dom/context.js"
    },
    "./background-worker": {
      "types": "./dist/entry-background-worker.d.ts",
      "mask-src": "./src/entry-background-worker.ts",
      "default": "./dist/entry-background-worker.js"
    },
    "./content-script": {
      "types": "./dist/entry-content-script.d.ts",
      "mask-src": "./src/entry-content-script.ts",
      "default": "./dist/entry-content-script.js"
    },
    "./content-script/context": {
      "types": "./dist/site-adaptor/context.d.ts",
      "mask-src": "./src/site-adaptor/context.ts",
      "default": "./dist/site-adaptor/context.js"
    }
  },
  "types": "./dist/entry.d.ts",
  "typesVersions": {
    "*": {
      "dom": [
        "./dist/entry-dom.d.ts"
      ],
      "background-worker": [
        "./dist/entry-background-worker.d.ts"
      ],
      "dashboard": [
        "./dist/entry-dashboard.d.ts"
      ],
      "content-script": [
        "./dist/entry-content-script.d.ts"
      ]
    }
  },
  "dependencies": {
    "@masknet/encryption": "workspace:^",
    "@masknet/flags": "workspace:^",
    "@masknet/public-api": "workspace:^",
    "@masknet/shared-base": "workspace:^",
    "@masknet/shared-base-ui": "workspace:^",
    "@masknet/theme": "workspace:^",
    "@masknet/typed-message": "workspace:^",
    "@masknet/web3-helpers": "workspace:^",
    "@masknet/web3-shared-base": "workspace:^",
    "@servie/events": "^3.0.0",
    "@types/use-subscription": "^1.0.0",
    "async-call-rpc": "^6.4.0",
    "react-use": "^17.5.1",
    "use-subscription": "^1.8.0"
  }
}
