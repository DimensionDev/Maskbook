{
  "name": "@masknet/web3-providers",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "mask-src": "./src/entry.ts",
      "default": "./dist/entry.js"
    },
    "./helpers": {
      "types": "./dist/entry-helpers.d.ts",
      "mask-src": "./src/entry-helpers.ts",
      "default": "./dist/entry-helpers.js"
    },
    "./types": {
      "types": "./dist/entry-types.d.ts",
      "mask-src": "./src/entry-types.ts",
      "default": "./dist/entry-types.js"
    }
  },
  "types": "./dist/entry.d.ts",
  "typesVersions": {
    "*": {
      "helpers": [
        "./dist/entry-helpers.d.ts"
      ],
      "types": [
        "./dist/entry-types.d.ts"
      ]
    }
  },
  "dependencies": {
    "@masknet/flags": "workspace:^",
    "@masknet/icons": "workspace:^",
    "@masknet/injected-script": "workspace:^",
    "@masknet/shared-base": "workspace:^",
    "@masknet/shared-base-ui": "workspace:^",
    "@masknet/web3-constants": "workspace:^",
    "@masknet/web3-contracts": "workspace:^",
    "@masknet/web3-helpers": "workspace:^",
    "@masknet/web3-shared-base": "workspace:^",
    "@masknet/web3-shared-evm": "workspace:^",
    "@masknet/web3-shared-solana": "workspace:^",
    "@servie/events": "^3.0.0",
    "bignumber.js": "9.1.2",
    "bn.js": "^4.12.0",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "date-fns": "^2.30.0",
    "fuse.js": "^7.0.0",
    "immer": "^10.0.2",
    "lru-cache": "^10.0.0",
    "urlcat": "^3.1.0",
    "uuid": "^9.0.0",
    "web3-core": "1.10.2",
    "web3-core-helpers": "1.10.2",
    "web3-eth": "1.10.2",
    "web3-utils": "1.10.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/bs58": "^4.0.1",
    "@types/uuid": "^9.0.3"
  }
}
