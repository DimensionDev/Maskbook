    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gun": "0.2019.1211",
    "husky": "^3.1.0",
    "idb": "^4.0.5",
    "json-stable-stringify": "^1.0.1",
    "jsqr": "^1.2.0",
    "jss": "^10",
    "lint-staged": "^9.5.0",
    "lodash-es": "^4.17.11",
    "node-stego": "^0.8.1",
    "notistack": "^0.9.7",
    "npm-run-all": "^4.1.5",
    "pvtsutils": "^1.0.9",
    "qrcode": "^1.4.4",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.3.0",
    "tiny-secp256k1": "^1.1.2",
