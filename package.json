{
  "name": "maskbook",
  "version": "1.36.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "dev",
    "start-e2e": "env target=E2E dev --preset E2E",
    "ts": "gulp codegen-watch",
    "codegen": "gulp codegen-watch",
    "go": "dev --help",
    "build": "build",
    "build-e2e": "env target=E2E build --preset E2E",
    "build-ios": "build --preset iOS",
    "lint": "eslint -c packages/.eslintrc.json packages --ext .ts,.tsx,.js --cache --fix",
    "lint:ci": "eslint -c packages/.eslintrc.json packages --ext .ts,.tsx,.js --cache",
    "test": "jest --verbose --watch",
    "test:ci": "jest --verbose --ci --collectCoverage=true --reporters=default --reporters=jest-junit -w 1 --forceExit",
    "test:e2e": "jest --verbose --runInBand --config=jest-e2e.config.js",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "packages/**/*.{ts,tsx,js,jsx}": "eslint -c packages/.eslintrc.json --fix",
    "packages/maskbook/**/*": "node ./packages/scripts/src/bin/precommit-hook-localekit.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/serialize": "^1.0.2",
    "@emotion/styled": "^11.3.0",
    "@emotion/utils": "^1.0.0",
    "@material-ui/core": "5.0.0-alpha.36",
    "@material-ui/icons": "5.0.0-alpha.35",
    "@material-ui/lab": "5.0.0-alpha.36",
    "@material-ui/system": "5.0.0-alpha.36",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/web": "^0.0.16",
    "i18next": "^20.3.4",
    "idb": "5.0.8",
    "react": "=18.0.0-alpha-aecb3b6d1",
    "react-dom": "=18.0.0-alpha-aecb3b6d1",
    "react-i18next": "^11.11.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@dimensiondev/patch-package": "^6.5.0",
    "@magic-works/commonjs-import.meta": "^1.0.1",
    "@magic-works/i18n-codegen": "^0.0.6",
    "@masknet/cli": "workspace:*",
    "@nice-labs/git-rev": "^3.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-unused-imports": "^1.1.3",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "ttypescript": "^1.5.12",
    "typescript": "4.4.1-rc"
  },
  "engines": {
    "node": ">=14.4.0",
    "pnpm": ">=6"
  }
}
